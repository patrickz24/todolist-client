/TODOList/todolist-client/src/reducers/auth.js",[],"/home/patrick/Bureau/TODOList/todolist-client/src/actions/index.js",[],"/home/patrick/Bureau/TODOList/todolist-client/src/actions/auth.js",[],"/home/patrick/Bureau/TODOList/todolist-client/src/apiCall/index.js",[],"/home/patrick/Bureau/TODOList/todolist-client/src/context/authContext.jsx",[],"/home/patrick/Bureau/TODOList/todolist-client/src/context/createDataContext.jsx",["87"],"import React, {useReducer, createContext} from 'react';\n\n\nexport default (reducer, actions, initialState) => {\nconst Context = createContext();\n\nconst Provider = ({children}) => {\nconst [state, dispatch]= useReducer(reducer, initialState);\n// actions = { signUp : (dispatch) => (user) => {}}\nconst boundActions= {};\nfor (let key in actions){\nboundActions[key] = actions[key](dispatch);\n}\n\n\n    return (\n        <Context.Provider value={{state, ...boundActions}}>\n            {children}\n            </Context.Provider>\n    );\n}\nreturn { Context, Provider};\n};","/home/patrick/Bureau/TODOList/todolist-client/src/components/layouts/NavBar.jsx",["88","89"],"/home/patrick/Bureau/TODOList/todolist-client/src/components/layouts/Styles.js",[],"/home/patrick/Bureau/TODOList/todolist-client/src/components/auth/SignUp.jsx",["90","91"],"import React, { useState, useContext } from 'react';\nimport classnames from 'classnames';\nimport { NavLink } from 'react-router-dom';\nimport Layout from '../layouts/Layout';\nimport { AuthFormWrapper } from './Styles';\nimport { Context } from '../../context/authContext';\n\nconst SignUp = ({ signUp }) => {\n  const { state } = useContext(Context);\n  const [first_name, setFirst_name] = useState('');\n  const [last_name, setLast_name] = useState('');\n  const [email, setemail] = useState('');\n  const [password, setpassword] = useState('');\n   const [first_nameErr, setFirst_nameErr] = useState('');\n   const [last_nameErr, setLast_nameErr] = useState('');\n  const [emailErr, setemailErr] = useState('');\n  const [passwordErr, setpasswordErr] = useState('');\n \n\n  const handleChange = (e, name) => {\n    const user = {};\n    const emailRegEx = RegExp(\n      /^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$/\n    );\n    user[name] = e.target.value;\n    // validations\n    switch (name) {\n      case 'first_name':\n        setFirst_name(user.first_name);\n        user.first_name.length < 3 ? setFirst_nameErr('Name must be at least 3 characters!') : setFirst_nameErr('');\n        break;\n        case 'last_name':\n            setLast_name(user.last_name);\n            user.last_name.length < 3 ? setLast_nameErr('Name must be at least 3 characters!') : setLast_nameErr('');\n            break;\n      case 'email':\n        setemail(user.email);\n        !emailRegEx.test(user.email) ? setemailErr('Invalid Email!') : setemailErr('');\n        break;\n      case 'password':\n        setpassword(user.password);\n        user.password.length < 8 ? setpasswordErr('Password must be at least 8 characters!') : setpasswordErr('');\n        break;\n        default:\n        break;\n    }\n  }\n\n  const handleSignUp = async (e) => {\n    e.preventDefault();\n    if (first_name && last_name && email && password &&  !first_nameErr && !last_nameErr && !emailErr && !passwordErr) {\n      await signUp({ first_name, last_name, email, password });\n    }\n  }\n\n  return (\n    <Layout>\n      <AuthFormWrapper>\n        <h2 className=\"text-center\">Inscription</h2>\n        {state.signUpErr && <div className=\"alert alert-danger text-center\">\n          <span className=\"text-danger text-capitalize\">{state.signUpErr.description}</span>\n          </div>}\n        <form className=\"mt-4\" onSubmit={handleSignUp}>\n          <div className=\"form-group\">\n            <label htmlFor=\"first_name\">Pr√©nom</label>\n            <input\n              type=\"text\"\n              name=\"first_name\"\n              className={classnames(\n                \"form-control\",\n                { 'is-invalid': first_nameErr, 'is-valid': !first_nameErr && first_name.length }\n              )}\n              id=\"first_name\"\n              placeholder=\"Pr√©nom\"\n              onChange={(e) => handleChange(e, 'first_name')}\n            />\n            {first_nameErr && <small className=\"text-danger\">{first_nameErr}</small>}\n          </div>\n\n          <div className=\"form-group\">\n            <label htmlFor=\"last_name\">Nom</label>\n            <input\n              type=\"text\"\n              name=\"last_name\"\n              className={classnames(\n                \"form-control\",\n                { 'is-invalid': last_nameErr, 'is-valid': !last_nameErr && last_name.length }\n              )}\n              id=\"last_name\"\n              placeholder=\"Nom\"\n              onChange={(e) => handleChange(e, 'last_name')}\n            />\n            {last_nameErr && <small className=\"text-danger\">{last_nameErr}</small>}\n          </div>\n\n          <div className=\"form-group\">\n            <label htmlFor=\"email\">Email</label>\n            <input\n              type=\"email\"\n              name=\"email\"\n              className={classnames(\n                \"form-control\",\n                { 'is-invalid': emailErr, 'is-valid': !emailErr && email.length }\n              )}\n              id=\"email\"\n              placeholder=\"Email\"\n              onChange={(e) => handleChange(e, 'email')}\n            />\n            {emailErr && <small className=\"text-danger\">{emailErr}</small>}\n          </div>\n          <div className=\"form-group\">\n            <label htmlFor=\"password\">Mot de passe</label>\n            <input\n              type=\"password\"\n              name=\"password\"\n              className={classnames(\n                \"form-control\",\n                { 'is-invalid': passwordErr, 'is-valid': !passwordErr && password.length }\n              )}\n              id=\"password\"\n              placeholder=\"Mot de passe\"\n              onChange={(e) => handleChange(e, 'password')}\n            />\n            {passwordErr && <small className=\"text-danger\">{passwordErr}</small>}\n          </div>\n          \n          <input type=\"submit\" className=\"btn btn-primary\" value=\"S'inscrire\"/>\n        </form>\n        <p className=\"float-left\">\n          Vous souhaitez vous connecter? <NavLink to=\"signin\">Connexion</NavLink>\n        </p>\n      </AuthFormWrapper>\n    </Layout>\n  )\n}\n\nexport default SignUp","/home/patrick/Bureau/TODOList/todolist-client/src/components/layouts/Layout.jsx",[],"/home/patrick/Bureau/TODOList/todolist-client/src/components/auth/Styles.js",[],"/home/patrick/Bureau/TODOList/todolist-client/src/components/layouts/Footer.js",[],"/home/patrick/Bureau/TODOList/todolist-client/src/components/auth/SignIn.jsx",["92","93"],"/home/patrick/Bureau/TODOList/todolist-client/src/components/todos/ListTodos.jsx",[],"/home/patrick/Bureau/TODOList/todolist-client/src/components/todos/CreateTodoModal.jsx",["94"],"/home/patrick/Bureau/TODOList/todolist-client/src/components/todos/Styles.js",[],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":4,"column":1,"nodeType":"101","endLine":23,"endColumn":3},{"ruleId":"102","severity":1,"message":"103","line":19,"column":4,"nodeType":"104","endLine":19,"endColumn":6,"suggestions":"105"},{"ruleId":"106","severity":1,"message":"107","line":52,"column":7,"nodeType":"108","messageId":"109","endLine":52,"endColumn":22},{"ruleId":"110","severity":1,"message":"111","line":23,"column":17,"nodeType":"112","messageId":"113","endLine":23,"endColumn":18,"suggestions":"114"},{"ruleId":"110","severity":1,"message":"111","line":23,"column":42,"nodeType":"112","messageId":"113","endLine":23,"endColumn":43,"suggestions":"115"},{"ruleId"